<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wx.springboot.system.dao.DictItemDao">
    <select id="getByText" resultType="int">
        SELECT COUNT(*)
        FROM sys_dict_item i
        WHERE i.code = #{code} AND i.text = #{text}
    </select>

</mapper>